from django.db import models\
from django.contrib.auth.models import AbstractUser\
\
# Kullan\uc0\u305 c\u305  Modeli\
class User(AbstractUser):\
    ROLE_CHOICES = [\
        ('expert', 'Kalite Uzman\uc0\u305 '),\
        ('agent', 'M\'fc\uc0\u351 teri Temsilcisi'),\
        ('admin', 'Y\'f6netici')\
